{
	"$schema": "https://schemas.wp.org/wp/6.3/theme.json",
	"customTemplates": [],
	"settings": {
		"appearanceTools": true,
		"color": {
			"background": true,
			"custom": true,
			"customDuotone": true,
			"customGradient": true,
			"defaultDuotone": true,
			"defaultGradients": true,
			"defaultPalette": true,
			"duotone": [],
			"gradients": [],
			"palette": [
				{
					"color": "#ffffff",
					"name": "Base",
					"slug": "base"
				},
				{
					"color": "#272E35",
					"name": "Primary",
					"slug": "primary"
				},
				{
					"color": "#3069B3",
					"name": "Blue",
					"slug": "blue"
				},
				{
					"color": "#113A6F",
					"name": "Dark blue",
					"slug": "dark-blue"
				},
				{
					"color": "#EFF6FF",
					"name": "Light blue",
					"slug": "light-blue"
				},
				{
					"color": "#FFF5EE",
					"name": "Light orange",
					"slug": "light-orange"
				},
				{
					"color": "#FFF9EE",
					"name": "Yellow",
					"slug": "yellow"
				},
				{
					"color": "#555F6D",
					"name": "Grey",
					"slug": "grey"
				},
				{
					"color": "#E9ECEF",
					"name": "Light grey",
					"slug": "light-grey"
				}
			],
			"text": true
		},
		"layout": {
			"contentSize": "1280px",
			"wideSize": "1280px"
		},
		"shadow": {
			"defaultPresets": true,
			"presets": []
		},
		"spacing": {
			"customSpacingSize": true,
			"spacingSizes": [
				{
					"name": "0.5rem",
					"size": "clamp(0.25rem, 5vw, 0.5rem)",
					"slug": "10"
				},
				{
					"name": "1rem",
					"size": "clamp(0.5rem, 5vw, 1rem)",
					"slug": "20"
				},
				{
					"name": "1.5rem",
					"size": "clamp(0.75rem, 5vw, 1.5rem)",
					"slug": "30"
				},
				{
					"name": "2rem",
					"size": "clamp(1rem, 5vw, 2rem)",
					"slug": "40"
				},
				{
					"name": "2.5rem",
					"size": "clamp(1.25rem, 5vw, 2.5rem)",
					"slug": "50"
				},
				{
					"name": "3rem",
					"size": "clamp(1.5rem, 5vw, 3rem)",
					"slug": "60"
				},
				{
					"name": "4rem",
					"size": "clamp(2rem, 5vw, 4rem)",
					"slug": "70"
				}
			],
			"units": [
				"%",
				"px",
				"em",
				"rem",
				"vh",
				"vw"
			]
		},
		"typography": {
			"customFontSize": true,
			"dropCap": false,
			"fluid": true,
			"fontFamilies": [
				{
					"fontFamily": "\"Inter\", sans-serif",
					"name": "Inter",
					"slug": "inter"
				}
			],
			"fontSizes": [
				{
					"fluid": false,
					"name": "Extra Small",
					"size": "0.75rem",
					"slug": "x-small"
				},
				{
					"fluid": false,
					"name": "Small",
					"size": "0.875rem",
					"slug": "small"
				},
				{
					"fluid": false,
					"name": "Medium",
					"size": "1rem",
					"slug": "medium"
				},
				{
					"fluid": false,
					"name": "Large",
					"size": "1.125rem",
					"slug": "large"
				},
				{
					"fluid": false,
					"name": "Extra Large",
					"size": "1.25rem",
					"slug": "x-large"
				},
				{
					"fluid": {
						"max": "2.375rem",
						"min": "1.75rem"
					},
					"name": "H1",
					"size": "2.375rem",
					"slug": "h1"
				},
				{
					"fluid": {
						"max": "2.125rem",
						"min": "1.5rem"
					},
					"name": "H2",
					"size": "2.125rem",
					"slug": "h2"
				},
				{
					"fluid": {
						"max": "1.875rem",
						"min": "1.375rem"
					},
					"name": "H3",
					"size": "1.875rem",
					"slug": "h3"
				},
				{
					"fluid": {
						"max": "1.625rem",
						"min": "1.25rem"
					},
					"name": "H4",
					"size": "1.625rem",
					"slug": "h4"
				},
				{
					"fluid": {
						"max": "1.375rem",
						"min": "1.125rem"
					},
					"name": "H5",
					"size": "1.375rem",
					"slug": "h5"
				}
			],
			"fontStyle": true,
			"fontWeight": true,
			"letterSpacing": true,
			"textDecoration": true,
			"textTransform": true
		},
		"useRootPaddingAwareAlignments": true
	},
	"styles": {
		"blocks": {
			"core/list": {
				"typography": {
					"lineHeight": "2"
				},
				"spacing": {
					"padding": {
						"left": "1.5rem"
					}
				}
			},
			"core/navigation": {
				"elements": {
					"link": {
						":active": {
							"color": {
								"text": "var(--wp--preset--color--blue)!important"
							},
							"typography": {
								"textDecoration": "none"
							}
						},
						":focus": {
							"color": {
								"text": "var(--wp--preset--color--blue)!important"
							},
							"typography": {}
						},
						":hover": {
							"color": {
								"text": "var(--wp--preset--color--blue)!important"
							},
							"typography": {
								"textDecoration": "none"
							}
						},
						"typography": {
							"textDecoration": "none"
						}
					}
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--medium)",
					"lineHeight": "2rem"
				}
			},
			"core/paragraph": {
				"typography": {
					"lineHeight": "2"
				}
			},
			"core/post-title": {
				"elements": {
					"link": {
						":active": {
							"color": {
								"text": "var(--wp--preset--color--secondary)"
							},
							"typography": {
								"textDecoration": "none"
							}
						},
						":focus": {
							"typography": {
								"textDecoration": "underline"
							}
						},
						":hover": {
							"typography": {
								"textDecoration": "underline"
							}
						},
						"typography": {
							"textDecoration": "none"
						}
					}
				},
				"spacing": {
					"margin": {
						"bottom": "1.25rem",
						"top": "1.25rem"
					}
				},
				"typography": {
					"fontWeight": "400"
				}
			},
			"core/query": {
				"elements": {
					"h2": {
						"typography": {
							"fontSize": "var(--wp--preset--font-size--x-large)"
						}
					}
				}
			},
			"core/query-pagination": {
				"elements": {
					"link": {
						":hover": {
							"typography": {
								"textDecoration": "underline"
							}
						},
						"typography": {
							"textDecoration": "none"
						}
					}
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)",
					"fontWeight": "400"
				}
			},
			"core/separator": {
				"css": " &:not(.is-style-wide):not(.is-style-dots):not(.alignwide):not(.alignfull){width: 100px}"
			},
			"core/site-title": {
				"elements": {
					"link": {
						":active": {
							"color": {
								"text": "var(--wp--preset--color--secondary)"
							},
							"typography": {
								"textDecoration": "none"
							}
						},
						":focus": {
							"typography": {
								"textDecoration": "underline"
							}
						},
						":hover": {
							"typography": {
								"textDecoration": "underline"
							}
						},
						"typography": {
							"textDecoration": "none"
						}
					}
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--medium)",
					"fontWeight": "normal",
					"lineHeight": "1.4"
				}
			}
		},
		"color": {
			"background": "var(--wp--preset--color--base)",
			"text": "var(--wp--preset--color--primary)"
		},
		"elements": {
			"button": {
				":active": {},
				":focus": {},
				":hover": {
					"color": {
						"background": "var(--wp--preset--color--dark-blue)",
						"text": "var(--wp--preset--color--base)"
					},
					"typography": {
						"textDecoration": "none"
					}
				},
				":visited": {},
				"color": {
					"background": "var(--wp--preset--color--blue)",
					"text": "var(--wp--preset--color--base)"
				},
				"spacing": {
					"padding": {
						"bottom": "var(--wp--preset--spacing--10)!important",
						"left": "var(--wp--preset--spacing--20)!important",
						"right": "var(--wp--preset--spacing--20)!important",
						"top": "var(--wp--preset--spacing--10)!important"
					}
				},
				"typography": {
					"fontSize":  "var(--wp--preset--font-size--small)"
				},
				"border": {
					"radius": {
						"topLeft": "0.5rem",
						"bottomLeft": "0.5rem",
						"bottomRight": "0.5rem",
						"topRight": "0.5rem"
					}
				}
			},
			"caption": {
				"color": {
					"text": "var(--wp--preset--color--grey)"
				},
				"spacing": {
					"margin": {
						"top": "1rem"
					}
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--x-small)",
					"fontStyle": "italic"
				}
			},
			"h1": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--h-1)",
					"lineHeight": "1.5"
				}
			},
			"h2": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--h-2)",
					"lineHeight": "1.2"
				}
			},
			"h3": {
				"spacing": {
					"margin": {
						"top": "2rem"
					}
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--h-3)"
				}
			},
			"h4": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--h-4)"
				}
			},
			"h5": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--h-5)"
				}
			},
			"h6": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--h-6)"
				}
			},
			"heading": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--inter)",
					"fontWeight": "600",
					"lineHeight": "1.4"
				}
			},
			"link": {
				":active": {
					"color": {
						"text": "var(--wp--preset--color--secondary)"
					},
					"typography": {}
				},
				":focus": {
					"typography": {}
				},
				":hover": {
					"typography": {
						"textDecoration": "underline"
					}
				},
				"color": {
					"text": "var(--wp--preset--color--contrast)"
				},
				"typography": {
					"textDecoration": "none"
				}
			}
		},
		"spacing": {
			"blockGap": "1rem",
			"padding": {
				"bottom": "0",
				"left": "var(--wp--preset--spacing--30)",
				"right": "var(--wp--preset--spacing--30)",
				"top": "0"
			}
		},
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--inter)",
			"fontSize": "var(--wp--preset--font-size--medium)",
			"lineHeight": "1.6",
			"letterSpacing": "0.2px"
		}
	},
	"templateParts": [
		{
			"area": "header",
			"name": "header",
			"title": "Header" 
		},
		{
			"area": "footer",
			"name": "footer",
			"title": "Footer"
		}
	],
	"version": 2
}